function factorial(n) {
	// write your code here
    let answer = 1;
    if (n == 0 || n == 1){
        return answer;
    }else{
        for(var i = n; i >= 1; i--){
            answer = answer * i;
        }
    }
	return answer;
}

let n = 4;
console.log("The factorial of " + n + " is " + factorial(n));
